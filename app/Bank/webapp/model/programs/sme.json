{
  "programId": "sme",
  "programName": "SME Development Fund",
  "description": "Financial support for small and medium enterprises looking to expand operations, increase capacity, or enter new markets.",
  "fundingRange": {
    "min": 20000000,
    "max": 200000000,
    "currency": "RWF"
  },
  "eligibility": {
    "businessAge": {
      "min": 2,
      "unit": "years"
    },
    "registrationRequired": true,
    "country": ["RW"],
    "requirements": [
      "Operating for at least 2 years",
      "Positive cash flow",
      "Clear expansion plans",
      "Audited financial statements"
    ]
  },
  "steps": {
    "programSelection": {
      "enabled": true,
      "required": true
    },
    "applicantInfo": {
      "enabled": true,
      "required": true,
      "fields": {
        "fullName": {"required": true, "visible": true},
        "email": {"required": true, "visible": true},
        "phone": {"required": true, "visible": true},
        "dateOfBirth": {"required": true, "visible": true},
        "address": {"required": true, "visible": true},
        "city": {"required": true, "visible": true},
        "country": {"required": true, "visible": true},
        "postalCode": {"required": true, "visible": true}
      }
    },
    "projectDetails": {
      "enabled": true,
      "required": true,
      "fields": {
        "title": {"required": true, "visible": true},
        "description": {"required": true, "visible": true, "minLength": 150},
        "fundingAmount": {"required": true, "visible": true},
        "duration": {"required": true, "visible": true, "max": 60},
        "category": {"required": true, "visible": true},
        "startDate": {"required": true, "visible": true}
      },
      "documents": {
        "businessPlan": {"required": true, "maxSize": 10, "formats": ["pdf", "doc", "docx"]},
        "financialStatements": {"required": true, "maxSize": 5, "formats": ["pdf", "xls", "xlsx"]},
        "taxClearance": {"required": true, "maxSize": 3, "formats": ["pdf"]},
        "companyRegistration": {"required": true, "maxSize": 3, "formats": ["pdf"]}
      }
    },
    "review": {
      "enabled": true,
      "termsAndConditions": true
    }
  },
  "notifications": {
    "email": true,
    "sms": true
  },
  "processingTime": {
    "value": 60,
    "unit": "days"
  }
}
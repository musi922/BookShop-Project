{
  "name": "study-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ui5 serve",
    "build": "npm run clean && npm run zip",
    "clean": "npx rimraf Study-app-content.zip",
    "zip": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo"
  },
  "devDependencies": {
    "@sap/ux-ui5-tooling": "^1",
    "@ui5/cli": "^3",
    "@ui5/fs": "^3",
    "@ui5/builder": "^3",
    "ui5-task-zipper": "^3.4.1"
  }
}